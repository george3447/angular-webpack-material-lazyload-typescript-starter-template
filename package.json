        "analyseBuild": "webpack --profile --json > status.json",
        "test": "echo \"Error: no test specified\" && exit 1",
        "cleanjs": "rimraf src/app/*.js, src/app/**/*.js, src/app/**/**/*.js",
        "webpack-defaults": "webpack-defaults"
